            <method name="open"><![CDATA[productReportService = new Packages.com.elasticpath.cmclient.reporting.lowstock.services.impl.InventoryLowStockReportServiceImpl;
java.lang.System.out.println(productReportService);

stock = productReportService.lowStockReport();
java.lang.System.out.println(stock);

totalRows = stock.size();

currentRow = 0;]]></method>
            <method name="fetch"><![CDATA[if (currentRow >= totalRows) {
    return false;
}
row["currentRowNum"] = currentRow;

var product = stock.get(currentRow);

currentRow++;
row["Brand"] = product[0];
row["SKU Code"] = product[1];
row["Product Name"] = product[2];
row["Quantity On Hand"] = product[3];
row["Available Quantity"] = product[4];
row["Quantity Allocated"] = product[5];
row["Reserved Quantity"] = product[6];
row["Re-Order Minimum"] = product[7];
row["Re-Order Quantity"] = product[8];
row["Expected Re-Stock Date"] = product[9];

return true;]]></method>
            <method name="close"><![CDATA[productReportService = null;
stock = null;

totalRows = null;
currentRow = null;]]></method>
