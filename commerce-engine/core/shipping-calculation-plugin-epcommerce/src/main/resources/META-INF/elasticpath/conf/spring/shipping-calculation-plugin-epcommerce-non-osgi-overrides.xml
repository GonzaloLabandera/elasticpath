<?xml version="1.0" encoding="UTF-8"?>
<beans
		xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="
				http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd"
		profile="default,!disable-shipping-calculation-plugin-epcommerce">

	<!-- overriding priced/unpriced shipping calculation plugin list -->
	<alias alias="unpricedShippingCalculationPluginList" name="epUnpricedShippingCalculationPluginList"/>
	<alias alias="pricedShippingCalculationPluginList" name="epPricedShippingCalculationPluginList"/>

</beans>
