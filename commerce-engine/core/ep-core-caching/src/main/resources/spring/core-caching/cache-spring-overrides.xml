<?xml version="1.0" encoding="UTF-8"?>
<beans
		xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
		">

	<!--
		Spring service bean overrides for non-OSGI environments.

		In OSGI environments this should be done via a reference-listener in core-blueprint.xml
	-->
	<bean factory-bean="categoryLookupProxy" factory-method="bindImplementation">
		<constructor-arg ref="cachingCategoryLookup"/>
	</bean>

	<bean factory-bean="productCharacteristicsServiceProxy" factory-method="bindImplementation">
		<constructor-arg ref="cachingProductCharacteristicsService"/>
	</bean>

	<bean factory-bean="productLookupProxy" factory-method="bindImplementation">
		<constructor-arg ref="cachingProductLookup"/>
	</bean>

	<bean factory-bean="ProductSkuLookupProxy" factory-method="bindImplementation">
		<constructor-arg ref="cachingProductSkuLookup"/>
	</bean>

	<bean factory-bean="storeServiceProxy" factory-method="bindImplementation">
		<constructor-arg ref="cachingStoreService"/>
	</bean>

	<bean factory-bean="priceAdjustmentServiceProxy" factory-method="bindImplementation">
		<constructor-arg ref="cachingPriceAdjustmentService"/>
	</bean>

	<bean factory-bean="ruleServiceProxy" factory-method="bindImplementation">
		<constructor-arg ref="cachingRuleService"/>
	</bean>

	<bean factory-bean="priceListAssignmentServiceProxy" factory-method="bindImplementation">
		<constructor-arg ref="cachingPriceListAssignmentService"/>
	</bean>

	<bean factory-bean="baseAmountFinderProxy" factory-method="bindImplementation">
		<constructor-arg ref="cachingBaseAmountFinder"/>
	</bean>

	<bean factory-bean="categoryServiceProxy" factory-method="bindImplementation">
		<constructor-arg ref="cachingCategoryService"/>
	</bean>
	<bean factory-bean="cachingFilteredNavigationConfigurationLoaderProxy" factory-method="bindImplementation">
		<constructor-arg ref="cachingFacetConfigurationLoader"/>
	</bean>
	<bean factory-bean="cachingSolrQueryFactoryProxy" factory-method="bindImplementation">
		<constructor-arg ref="cachingSolrQueryFactory"/>
	</bean>
	<bean factory-bean="facetServiceProxy" factory-method="bindImplementation">
		<constructor-arg ref="cachingFacetService"/>
	</bean>

	<bean factory-bean="skuOptionServiceProxy" factory-method="bindImplementation">
		<constructor-arg ref="cachingSkuOptionService"/>
	</bean>



	<bean factory-bean="attributeServiceProxy" factory-method="bindImplementation">
		<constructor-arg ref="cachingAttributeService"/>
	</bean>

	<bean factory-bean="brandServiceProxy" factory-method="bindImplementation">
		<constructor-arg ref="cachingBrandService"/>
	</bean>

	<bean factory-bean="productTypeDaoProxy" factory-method="bindImplementation">
		<constructor-arg ref="cachingProductTypeDao"/>
	</bean>

	<bean factory-bean="catalogServiceProxy" factory-method="bindImplementation">
		<constructor-arg ref="cachingCatalogService"/>
	</bean>

	<bean factory-bean="taxCodeServiceProxy" factory-method="bindImplementation">
		<constructor-arg ref="cachingTaxCodeService"/>
	</bean>

	<bean factory-bean="attributeValueValidationServiceProxy" factory-method="bindImplementation">
		<constructor-arg ref="cachingAttributeValueValidationService"/>
	</bean>

	<bean factory-bean="modifierFieldValidationServiceProxy" factory-method="bindImplementation">
		<constructor-arg ref="cachingModifierFieldValidationService"/>
	</bean>
</beans>